{
  "name": "@pr-gram/database",
  "version": "1.0.0",
  "description": "Database layer with Prisma",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rimraf dist",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seeds/index.ts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@pr-gram/shared": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "prisma": "^5.7.1",
    "typescript": "^5.3.3",
    "tsx": "^4.6.2",
    "rimraf": "^5.0.5"
  }
}